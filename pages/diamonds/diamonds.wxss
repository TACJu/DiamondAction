page {
  width: 100%;
  height: 100%
}

.pic{
  width: 100%;
  height: 400rpx;
}

.list-title{
  background-color:floralwhite; 
  border-bottom: 1rpx solid #e9e9e9;
  font-size: 40rpx;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  align-items: center;
  justify-content: center;
  align-content: center;
}
/* .list-title view{
  width:50%;
  display:inline-block;
} */
.right{
  text-align:right;
  padding-right:130rpx;
  color: gold;
  font-size: 60rpx;
}
.diamonds-item{
    width: 100%;
    border-bottom: 1px solid #eee;
    height: 200rpx;
    line-height: 200rpx;
    padding-left: 70rpx;
    font-size:40rpx;
}

.version {
  width:100%;
  height: 40rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	text-align: center;  
  padding: 40rpx;
  color: black;
}
.address-box{
    display: block;
    border-bottom: 20rpx solid #ededed;
    color: #999;
    line-height: 120rpx;
    font-size: 40rpx;
    height: 120rpx;
    background-color:floralwhite;
}
.address-box .address-manage{
    display: block;
    position: relative;
    height: 120rpx;
    overflow: scroll;
    border-bottom: 1rpx solid #e9e9e9;
    text-align: center;
    background-color:floralwhite;
    color: black;
}